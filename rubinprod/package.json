{
    "homepage": "https://Nepokornyi.github.io/Rubinproduction_3.0",
    "name": "rubinprod",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "predeploy": "yarn run build",
        "deploy": "gh-pages -d dist",
        "check:circular-dependencies": "madge --circular --no-spinner --exclude 'pages/.*(?=route.ts)' src/main.tsx",
        "check:code": "yarn check:type && yarn lint && yarn check:circular-dependencies && yarn check:dead-code",
        "check:dead-code": "knip",
        "check:type": "tsc --noEmit",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.4",
        "@types/three": "^0.162.0",
        "framer-motion": "^11.0.6",
        "gsap": "^3.12.5",
        "i18next": "^23.10.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.0",
        "react-i18next": "^14.0.5",
        "react-router-dom": "^6.22.1",
        "react-router-hash-link": "^2.4.3",
        "styled-components": "^6.1.8",
        "swiper": "^11.0.7",
        "three": "^0.162.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/react": "^18.2.56",
        "@types/react-dom": "^18.2.19",
        "@types/react-router-hash-link": "^2.4.9",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.1.1",
        "knip": "^5.0.2",
        "madge": "^6.1.0",
        "typescript": "^5.2.2",
        "vite": "^5.1.4"
    }
}
